generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Actividad {
  id          Int    @id @default(autoincrement())
  nombre      String
  descripcion String
  tipo        String
  horaInicio  String
  horaFin     String
  cupo        Int
}
